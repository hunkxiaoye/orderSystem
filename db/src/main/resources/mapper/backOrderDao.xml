<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.db.dao.backOrderDao">
    <select id="findBybackNumber" resultType="backOrder" parameterType="String">
        SELECT * FROM back_order WHERE back_number =#{back_number}
    </select>

    <insert id="add" parameterType="backOrder" >
        <selectKey keyProperty="id" order="AFTER" resultType="java.lang.Integer">
            SELECT LAST_INSERT_ID()
        </selectKey>
        INSERT INTO back_order(id,order_number,back_number,version,backstatus,createtime,updatetime,order_pay_number,amount,back_pay_status,refund_suc_time,user_id)
        VALUES
        (#{id},#{order_number},#{back_number},#{version},#{backstatus},#{createtime},#{updatetime},#{order_pay_number},#{amount},#{back_pay_status},#{refund_suc_time},#{user_id})
    </insert>

</mapper>

